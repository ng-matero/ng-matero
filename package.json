{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr --disable-host-check",
    "predeploy": "npm run build:prod",
    "deploy": "ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "husky install",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^17.3.9",
    "@angular/cdk": "^17.3.9",
    "@angular/common": "^17.3.9",
    "@angular/compiler": "^17.3.9",
    "@angular/core": "^17.3.9",
    "@angular/forms": "^17.3.9",
    "@angular/material": "^17.3.9",
    "@angular/material-experimental": "^17.3.9",
    "@angular/material-moment-adapter": "^17.3.9",
    "@angular/platform-browser": "^17.3.9",
    "@angular/platform-browser-dynamic": "^17.3.9",
    "@angular/router": "^17.3.9",
    "@ng-matero/extensions": "^17.3.2",
    "@ng-matero/extensions-moment-adapter": "^17.1.2",
    "@ngx-formly/core": "^6.3.0",
    "@ngx-formly/material": "^6.3.0",
    "@ngx-translate/core": "^15.0.0",
    "@ngx-translate/http-loader": "^8.0.0",
    "angular-in-memory-web-api": "~0.17.0",
    "apexcharts": "^3.48.0",
    "moment": "^2.30.0",
    "moment-timezone": "^0.5.45",
    "ngx-permissions": "^17.1.0",
    "ngx-progressbar": "^11.1.0",
    "ngx-toastr": "^18.0.0",
    "photoviewer": "^3.10.0",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.6.0",
    "zone.js": "~0.14.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^17.3.7",
    "@angular-eslint/builder": "^17.4.0",
    "@angular-eslint/eslint-plugin": "^17.4.0",
    "@angular-eslint/eslint-plugin-template": "^17.4.0",
    "@angular-eslint/schematics": "^17.4.0",
    "@angular-eslint/template-parser": "^17.4.0",
    "@angular/cli": "^17.3.7",
    "@angular/compiler-cli": "^17.3.7",
    "@commitlint/cli": "^19.3.0",
    "@commitlint/config-conventional": "^19.2.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^20.12.2",
    "@typescript-eslint/eslint-plugin": "^7.3.0",
    "@typescript-eslint/parser": "^7.3.0",
    "angular-cli-ghpages": "^2.0.0-beta.2",
    "cpr": "^3.0.1",
    "eslint": "^8.57.0",
    "gulp": "^4.0.2",
    "gulp-each": "^0.5.0",
    "husky": "^8.0.3",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^15.2.0",
    "parse5": "^7.1.2",
    "prettier": "^3.2.4",
    "protractor": "~7.0.0",
    "rimraf": "^5.0.0",
    "stylelint": "^16.5.0",
    "stylelint-config-recess-order": "^5.0.0",
    "stylelint-config-recommended-scss": "^14.0.0",
    "stylelint-config-standard": "^36.0.0",
    "ts-node": "~10.9.0",
    "typescript": "~5.4.2",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
