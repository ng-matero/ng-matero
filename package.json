{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "ng lint --fix && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "deploy": "npm run build && ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "is-ci || husky",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/cdk": "^21.0.2",
    "@angular/common": "^21.0.3",
    "@angular/compiler": "^21.0.3",
    "@angular/core": "^21.0.3",
    "@angular/forms": "^21.0.3",
    "@angular/material": "^21.0.2",
    "@angular/material-date-fns-adapter": "^21.0.2",
    "@angular/platform-browser": "^21.0.3",
    "@angular/router": "^21.0.3",
    "@ng-matero/extensions": "^21.1.3",
    "@ng-matero/extensions-date-fns-adapter": "^21.0.0",
    "@ngneat/overview": "7.0.0",
    "@ngx-formly/core": "^7.0.0",
    "@ngx-formly/material": "^7.0.0",
    "@ngx-translate/core": "^17.0.0",
    "@ngx-translate/http-loader": "^17.0.0",
    "@ngxpert/hot-toast": "6.1.0",
    "angular-in-memory-web-api": "~0.21.0",
    "apexcharts": "^5.3.0",
    "base64-js": "^1.5.1",
    "date-fns": "^4.1.0",
    "ngx-permissions": "^19.0.0",
    "ngx-progressbar": "^14.0.0",
    "photoviewer": "^3.11.1",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.8.0",
    "zone.js": "~0.16.0"
  },
  "devDependencies": {
    "@angular/build": "^21.0.2",
    "@angular/cli": "^21.0.2",
    "@angular/compiler-cli": "^21.0.3",
    "@commitlint/cli": "^20.3.0",
    "@commitlint/config-conventional": "^20.3.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^25.0.0",
    "angular-cli-ghpages": "^3.0.0",
    "angular-eslint": "^21.1.0",
    "cpr": "^3.0.1",
    "eslint": "^9.39.0",
    "gulp": "^5.0.0",
    "gulp-each": "^0.5.0",
    "husky": "^9.1.6",
    "is-ci": "^4.1.0",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.2.0",
    "parse5": "^8.0.0",
    "prettier": "^3.7.0",
    "protractor": "~7.0.0",
    "rimraf": "^6.0.0",
    "stylelint": "^16.26.0",
    "stylelint-config-recess-order": "^7.4.0",
    "stylelint-config-recommended-scss": "^16.0.0",
    "stylelint-config-standard": "^39.0.0",
    "stylelint-order": "^7.0.0",
    "typescript": "~5.9.3",
    "typescript-eslint": "^8.53.0",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
