{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "npm run lint:ts && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "hmr": "ng serve --hmr --disable-host-check",
    "predeploy": "npm run build:prod",
    "deploy": "ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "husky install",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.0.4",
    "@angular/cdk": "^14.0.4",
    "@angular/common": "^14.0.4",
    "@angular/compiler": "^14.0.4",
    "@angular/core": "^14.0.4",
    "@angular/flex-layout": "^14.0.0-beta.40",
    "@angular/forms": "^14.0.4",
    "@angular/localize": "^14.0.4",
    "@angular/material": "^14.0.4",
    "@angular/material-moment-adapter": "^14.0.4",
    "@angular/platform-browser": "^14.0.4",
    "@angular/platform-browser-dynamic": "^14.0.4",
    "@angular/router": "^14.0.4",
    "@ng-matero/extensions": "^14.0.0",
    "@ng-matero/extensions-moment-adapter": "^14.0.0",
    "@ngx-formly/core": "^5.12.5",
    "@ngx-formly/material": "^5.12.5",
    "@ngx-translate/core": "^14.0.0",
    "@ngx-translate/http-loader": "^7.0.0",
    "angular-in-memory-web-api": "~0.14.0",
    "apexcharts": "^3.35.0",
    "moment": "^2.29.4",
    "moment-timezone": "^0.5.34",
    "ngx-permissions": "^13.0.1",
    "ngx-progressbar": "^9.0.0",
    "ngx-toastr": "^15.0.0",
    "photoviewer": "^3.6.2",
    "rxjs": "~7.5.5",
    "screenfull": "^6.0.2",
    "tslib": "^2.4.0",
    "zone.js": "~0.11.6"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.1.0",
    "@angular-eslint/builder": "^14.0.0",
    "@angular-eslint/eslint-plugin": "^14.0.0",
    "@angular-eslint/eslint-plugin-template": "^14.0.0",
    "@angular-eslint/schematics": "^14.0.0",
    "@angular-eslint/template-parser": "^14.0.0",
    "@angular/cli": "^14.0.4",
    "@angular/compiler-cli": "^14.0.4",
    "@angular/language-service": "^14.0.4",
    "@commitlint/cli": "^17.0.3",
    "@commitlint/config-conventional": "^17.0.3",
    "@types/jasmine": "~4.0.3",
    "@types/jasminewd2": "~2.0.10",
    "@types/node": "^18.0.0",
    "@types/parse5": "^6.0.3",
    "@typescript-eslint/eslint-plugin": "^5.29.0",
    "@typescript-eslint/parser": "^5.29.0",
    "angular-cli-ghpages": "^1.0.0",
    "chalk": "^5.0.1",
    "cpr": "^3.0.1",
    "eslint": "^8.18.0",
    "gulp": "^4.0.2",
    "gulp-each": "^0.5.0",
    "husky": "^8.0.1",
    "jasmine-core": "~4.2.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.1.1",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "lint-staged": "^13.0.3",
    "parse5": "^6.0.1",
    "prettier": "^2.7.1",
    "protractor": "~7.0.0",
    "rimraf": "^3.0.2",
    "stylelint": "^14.9.0",
    "stylelint-config-rational-order": "^0.1.2",
    "stylelint-config-recommended-scss": "^6.0.0",
    "stylelint-config-standard": "^26.0.0",
    "stylelint-order": "^5.0.0",
    "stylelint-scss": "^4.2.0",
    "ts-node": "~10.8.0",
    "typescript": "~4.7.4",
    "webpack-bundle-analyzer": "^4.5.0"
  }
}
