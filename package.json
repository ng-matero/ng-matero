{
  "name": "ng-matero",
  "version": "0.0.0-NOT-USED",
  "engines": {
    "yarn": ">=1.22.0 <2",
    "npm": "Please use Yarn instead of NPM to install dependencies"
  },
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "test": "ng test",
    "test:ci": "ng test --browsers=ChromeHeadless --watch=false --code-coverage",
    "e2e": "ng e2e",
    "build": "ng build",
    "build:prod": "ng build --base-href=/ng-matero/",
    "build:schematics": "npm run copy:schematics && cd schematics && npm run build && cd .. && npm run build:starter",
    "build:starter": "gulp --gulpfile gulpfile.js",
    "copy:schematics": "npm run clean:schematics && cpr schematics dist/schematics",
    "clean:schematics": "rimraf dist/schematics",
    "lint": "ng lint --fix && npm run lint:scss",
    "lint:ts": "eslint \"src/**/*.ts\" --fix",
    "lint:scss": "stylelint \"src/**/*.scss\" --fix",
    "deploy": "npm run build:prod && ng deploy",
    "publish": "npm run build:schematics && cd dist/schematics && npm publish",
    "prepare": "is-ci || husky",
    "analyze": "ng build --stats-json && webpack-bundle-analyzer dist/ng-matero/stats.json"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^20.1.0",
    "@angular/cdk": "^20.1.0",
    "@angular/common": "^20.1.0",
    "@angular/compiler": "^20.1.0",
    "@angular/core": "^20.1.0",
    "@angular/forms": "^20.1.0",
    "@angular/material": "^20.1.0",
    "@angular/material-date-fns-adapter": "^20.1.0",
    "@angular/platform-browser": "^20.1.0",
    "@angular/router": "^20.1.0",
    "@ng-matero/extensions": "^20.2.0",
    "@ng-matero/extensions-date-fns-adapter": "^20.0.0",
    "@ngx-formly/core": "^7.0.0",
    "@ngx-formly/material": "^7.0.0",
    "@ngx-translate/core": "^16.0.0",
    "@ngx-translate/http-loader": "^16.0.0",
    "angular-in-memory-web-api": "~0.20.0",
    "apexcharts": "~4.5.0",
    "date-fns": "^4.1.0",
    "ngx-permissions": "^19.0.0",
    "ngx-progressbar": "^14.0.0",
    "ngx-toastr": "^19.0.0",
    "photoviewer": "^3.10.3",
    "rxjs": "~7.8.0",
    "screenfull": "^6.0.2",
    "tslib": "^2.8.0",
    "zone.js": "~0.15.0"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^20.1.0",
    "@angular/cli": "^20.1.0",
    "@angular/compiler-cli": "^20.1.0",
    "@commitlint/cli": "^19.8.0",
    "@commitlint/config-conventional": "^19.8.0",
    "@types/jasmine": "~5.1.0",
    "@types/node": "^24.0.0",
    "angular-cli-ghpages": "^2.0.3",
    "angular-eslint": "^20.0.0",
    "cpr": "^3.0.1",
    "eslint": "^9.28.0",
    "gulp": "^5.0.0",
    "gulp-each": "^0.5.0",
    "husky": "^9.1.6",
    "is-ci": "^4.1.0",
    "jasmine-core": "~5.1.0",
    "jasmine-spec-reporter": "~7.0.0",
    "karma": "~6.4.0",
    "karma-chrome-launcher": "~3.2.0",
    "karma-coverage": "~2.2.0",
    "karma-jasmine": "~5.1.0",
    "karma-jasmine-html-reporter": "~2.1.0",
    "lint-staged": "^16.1.0",
    "parse5": "^7.2.0",
    "prettier": "^3.5.3",
    "protractor": "~7.0.0",
    "rimraf": "^5.0.0",
    "stylelint": "^16.20.0",
    "stylelint-config-recess-order": "^6.0.0",
    "stylelint-config-recommended-scss": "^15.0.0",
    "stylelint-config-standard": "^38.0.0",
    "typescript": "~5.8.3",
    "typescript-eslint": "^8.34.0",
    "webpack-bundle-analyzer": "^4.10.0"
  }
}
